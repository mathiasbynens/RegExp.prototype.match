		"regexp",
		"es6",
		"ecmascript",
		"polyfill"
	],
	"licenses": [
		{
			"type": "MIT",
			"url": "http://mths.be/mit"
		}
	],
	"author": {
		"name": "Mathias Bynens",
		"url": "http://mathiasbynens.be/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mathiasbynens/RegExp.prototype.match.git"
	},
	"bugs": {
		"url": "https://github.com/mathiasbynens/RegExp.prototype.match/issues"
	},
	"files": [
		"LICENSE-MIT.txt",
		"regexp-prototype-match.js"
	],
	"directories": {
		"test": "tests"
	},
	"devDependencies": {
		"coveralls": "^2.11.1"
	},
	"scripts": {
